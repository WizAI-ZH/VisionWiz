{
  "name": "VisionWiz",
  "version": "1.2.0",
  "description": "威智的AI工具",
  "main": "main_bootstrap.js",
  "config": {
    "forge": "./forge.config.js"
  },
  "build": {
    "appId": "com.WizAiTool.2024101101",
    "productName": "WizAiTool",
    "files": [
      "maix_train/**/*",
      "py39/**/*",
      "static/**/*",
      "tools/**/*",
      "utils/**/*",
      "icons/**/*",
      "compiled/**/*",
      "loading.html",
      "mainpage.html",
      "preload_compiled.js",
      "windowManger_compiled.js", 
      "menu_compiled.js",
      "eventManager_compiled.js",
      "main_compiled.js",
      "node_modules/bytenode/**/*",
      {
        "from": "out",
        "to": "out",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "icons/visionwiz_logo.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true
    },
    "electronVersion": "29.4.6",
    "asar": true
  },
  "scripts": {
    "test": "vesi_test",
    "compile": "node compile.js",
    "start-forge": "electron-forge start",
    "start": "electron .",
    "start2": "nodemon --watch main.js --exec npm run build",
    "rebuild-nodepty": "electron-rebuild -f -w node-pty",
    "rebuild-rob": "electron-rebuild -f -w robotjs",
    "package": "npm run compile && electron-forge package",
    "package-onekey": "npm run compile && node package.js",
    "make": "npm run compile && electron-forge make"
  },
  "keywords": [
    "AI",
    "tool",
    "yolo",
    "image",
    "classifier",
    "object",
    "detection",
    "mobile_net"
  ],
  "author": "曹振威",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
